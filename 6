#include<stdio.h>

int main() {
    char ch = 'A';
    int i, j;

    for(i = 1; i <= 6; i++) {
        for(j = 1; j <= i; j++) {
            if(i % 2 == 1) {
                printf("%c ", ch);
                ch++;
            } else {
                printf("%c ", ch + 32);
                ch++;
            }
        }
        printf("\n");
    }

    return 0;
}

