// Program to compare two integers

#include <stdio.h>

 

int main() {

  int num1, num2;

  printf("Enter two integers: ");

  scanf("%d %d", &num1, &num2);

  if(num1 > num2) {

    printf("%d is greater than %d",num1,num2);

  }

  else if (num1 < num2) {

    printf(" %d is greater than %d", num2, num1);

  }

  else if (num1 == num2) {

    printf("%d is equal to %d", num1, num2);

  }

  else {

    printf("Enter a valid number.");

  }

  return 0;

}
