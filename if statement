// Program to find that the given number is greater or smaller that 10

#include <stdio.h>

 

int main() {

    int num;

    printf("Enter an integer : ");

    scanf("%d", &num);

    if (num > 10) {

      printf("%d is greater than 10.\n", num);

    }

    printf("Here, the execution of program completed.");

    return 0;

}
